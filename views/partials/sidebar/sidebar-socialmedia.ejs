<h2>Redes Sociales</h2>
<section class="socialmedia-sidebar">
        <% for(var i=0; i < socialmedia.length; i++){ %>
                <a href="<%= socialmedia[i].url %>" class="social-icon">
                        <i class="fa <%= socialmedia[i].icon %>"></i>
                </a>
        <% } %>
</section>
